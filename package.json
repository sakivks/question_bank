{
  "name": "question_bank",
  "version": "0.1.0",
  "description": "collection of questions, and auto generation of question paper",
  "main": "server.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "buildDev": "NODE_ENV=development babel src -d build",
    "build": "NODE_ENV=production babel src -d build",
    "buildComplete": "npm run build && webpack --config webpack.production.config.js -p --display-exclude",
    "autoBuild": "nodemon --watch src --exec 'npm run buildDev'",
    "start": "node index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sakivks/question_bank.git"
  },
  "keywords": [
    "question",
    "bank",
    "latex"
  ],
  "author": "Vikas",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sakivks/question_bank/issues"
  },
  "homepage": "https://github.com/sakivks/question_bank#readme",
  "dependencies": {
    "dotenv": "^2.0.0",
    "koa": "^2.0.0",
    "koa-bodyparser": "^3.2.0",
    "koa-convert": "^1.2.0",
    "koa-favicon": "^1.2.1",
    "koa-logger": "^1.3.0",
    "koa-router": "^7.0.1",
    "koa-static": "^2.0.0",
    "material-ui": "^0.16.5",
    "mongoose": "^4.7.4",
    "react": "^15.4.1",
    "react-dom": "^15.4.1",
    "react-motion": "^0.4.7",
    "react-router": "^3.0.0",
    "react-swipeable-views": "^0.9.2",
    "react-tap-event-plugin": "^2.0.1"
  },
  "devDependencies": {
    "webpack": "^1.14.0",
    "babel-cli": "^6.18.0",
    "babel-core": "^6.21.0",
    "babel-loader": "^6.2.10",
    "babel-polyfill": "^6.20.0",
    "babel-preset-latest": "^6.16.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-0": "^6.16.0",
    "koa-webpack-dev-middleware": "^1.3.0",
    "koa-webpack-hot-middleware": "^1.0.3",
    "nodemon": "^1.11.0"
  }
